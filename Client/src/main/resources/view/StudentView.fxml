<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane fx:id="rootPane" prefHeight="400.0" prefWidth="700.0" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exam.client.controller.StudentController">
    <BorderPane fx:id="menuPane" prefHeight="200.0" prefWidth="200.0">
      <center>
         <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
               <Label fx:id="waitingLabel" text="Select Numbers" />
               <Spinner fx:id="nr1" editable="true" />
               <Spinner fx:id="nr2" editable="true" />
               <Button fx:id="startGame" mnemonicParsing="false" onAction="#start" text="Send Numbers">
                  <VBox.margin>
                     <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                  </VBox.margin></Button>
            </children>
         </VBox>
      </center>
    </BorderPane>
</StackPane>
